{
  "name": "@prisma/dev",
  "version": "0.7.1",
  "description": "A local Prisma Postgres server for development and testing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./daemon": {
      "import": {
        "types": "./dist/daemon/client.d.ts",
        "default": "./dist/daemon/client.js"
      },
      "require": {
        "types": "./dist/daemon/client.d.cts",
        "default": "./dist/daemon/client.cjs"
      }
    },
    "./internal/db": {
      "import": {
        "types": "./dist/db.d.ts",
        "default": "./dist/db.js"
      },
      "require": {
        "types": "./dist/db.d.cts",
        "default": "./dist/db.cjs"
      }
    },
    "./internal/state": {
      "import": {
        "types": "./dist/state.d.ts",
        "default": "./dist/state.js"
      },
      "require": {
        "types": "./dist/state.d.cts",
        "default": "./dist/state.cjs"
      }
    }
  },
  "keywords": [
    "prisma",
    "postgres",
    "accelerate",
    "local",
    "development",
    "dev",
    "testing"
  ],
  "license": "ISC",
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.1",
    "@types/node": "22.15.21",
    "@types/pako": "2.0.3",
    "@types/proper-lockfile": "4.1.4",
    "pkg-types": "2.1.0",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "3.1.4",
    "env-paths": "3.0.0",
    "common-stuff": "^0.0.0"
  },
  "dependencies": {
    "@electric-sql/pglite": "0.3.2",
    "@electric-sql/pglite-socket": "0.0.6",
    "@electric-sql/pglite-tools": "0.2.7",
    "@hono/node-server": "1.14.2",
    "@prisma/get-platform": "6.8.2",
    "foreground-child": "3.3.1",
    "hono": "4.7.10",
    "http-status-codes": "2.3.0",
    "pako": "2.1.0",
    "pathe": "2.0.3",
    "proper-lockfile": "4.1.2",
    "read-last-lines-ts": "1.2.1",
    "sock-daemon": "1.4.2",
    "std-env": "3.9.0",
    "valibot": "1.1.0"
  },
  "scripts": {
    "build": "tsup",
    "check:exports": "attw . --pack --profile node16",
    "dev": "tsup --watch",
    "lint": "eslint --fix .",
    "test": "",
    "typecheck": "tsc --noEmit"
  }
}